# Node.js Server Configuration
PORT=8080

# API Authentication (for communication between ESP32 and Server)
HTTP_USERNAME=admin
HTTP_PASSWORD=admin

# OpenRouter Configuration (Primary AI Provider)
OPENROUTER_API_KEY=your_openrouter_key_here

# Text AI Configuration
TEXT_AI_MODEL=kwaipilot/kat-coder-pro:free

# Vision AI Configuration
IMAGE_OPENROUTER_API_KEY=your_openrouter_key_here
IMAGE_AI_MODEL=google/gemini-2.0-flash-exp:free

# Chat Configuration
CHAT_NAME=STUDENT_1

# WiFi Credentials (Fallback for ESP32)
WIFI_SSID=Your_SSID
WIFI_PASS=Your_Password

# Ngrok Configuration (Optional)
NGROK_AUTHTOKEN=your_ngrok_token_here
